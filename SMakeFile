#################################################
#
# SAS/C Makefile for "ListNetworks"
# for AmigaOS M68K SAS/C 6.59 Compiler
#
#################################################

OUTFILE=ListNetworks

OBJECTS=ListNetworks.o

all: $(OUTFILE)

clean:
	@delete $(OBJECTS)

$(OUTFILE): $(OBJECTS)
	sc MATH=STANDARD LIB:amiga.lib CHKABORT NOICONS TO $(OUTFILE) LINK $(OBJECTS)
